# OKONS 环境变量配置示例

# 应用基础配置
REACT_APP_APP_NAME=OKONS
REACT_APP_APP_VERSION=1.0.0

# 网络配置
REACT_APP_CHAIN_ID=196
REACT_APP_CHAIN_NAME="X Layer"
REACT_APP_RPC_URL=https://xlayerrpc.okx.com

# 合约地址
REACT_APP_DOMAINS_CONTRACT_ADDRESS=0x...
REACT_APP_REGISTRAR_CONTRACT_ADDRESS=0x...

# WalletConnect 项目ID (从 https://cloud.walletconnect.com 获取)
REACT_APP_WALLETCONNECT_PROJECT_ID=your_project_id_here

# API配置 (如果需要)
REACT_APP_API_BASE_URL=https://api.okons.com

# 分析和监控 (可选)
REACT_APP_GA_MEASUREMENT_ID=G-XXXXXXXXXX
REACT_APP_SENTRY_DSN=https://...

# 功能开关 (可选)
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_REPORTING=true

# 部署环境
REACT_APP_ENVIRONMENT=production
